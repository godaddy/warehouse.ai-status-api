{
  "ensure": true,
  "http": {
    "host": "0.0.0.0",
    "port": 0
  },
  "wrhs": {
    "uri": "https://my.warehouse.endpoint.com"
  },
  "datastar": {
    "config": {
      "user": "cassandra",
      "password": "cassandra",
      "keyspace": "warehouseai_status_api",
      "hosts": ["127.0.0.1"],
      "keyspaceOptions": {
        "class": "SimpleStrategy",
        "replication_factor": 1
      }
    }
  },
  "webhooks": {
    "concurrency": 5,
    "endpoints": {
      "https://example.com/webhooks": [
        "package1",
        "package2"
      ],
      "https://carpediem.com/webhooks": [
        "package2",
        "package3",
        "package4"
      ]
    },
    "timeout": 2000
  }
}
